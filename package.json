{
  "name": "react-suite-template",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dll": "node_modules/.bin/webpack --config build/dll.base.js",
    "start": "node_modules/.bin/webpack-dev-server --config build/webpack.dev.js",
    "prod": "node_modules/.bin/webpack --config build/webpack.prod.js",
    "build": "npm run dll; npm run prod",
    "test": "jest"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "redux": "^3.7.2"
  },
  "devDependencies": {
    "autoprefixer": "^7.2.3",
    "babel": "^6.23.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^22.0.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-preval": "^1.6.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "css-loader": "^0.28.7",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "file-loader": "^1.1.6",
    "html-webpack-plugin": "^2.30.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.0.3",
    "jsdom": "^11.5.1",
    "less": "^2.7.3",
    "less-loader": "^4.0.5",
    "postcss-loader": "^2.0.9",
    "react-hot-loader": "^3.1.3",
    "react-test-renderer": "^16.2.0",
    "style-loader": "^0.19.1",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7",
    "webpack-hot-middleware": "^2.21.0",
    "webpack-merge": "^4.1.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__jest__/_mocks__/fileMocks.js",
      "\\.(css|scss|less)$": "identity-obj-proxy",
      "^@reducers(.*)$": "<rootDir>/src/reducers/$1",
      "^@components(.*)$": "<rootDir>/src/components/$1",
      "^@assets(.*)$": "<rootDir>/src/assets/$1",
      "^@utils(.*)$": "<rootDir>/src/utils/$1",
      "^@$": "<rootDir>/src/$1"
    },
    "collectCoverage": true,
    "coverageDirectory": "<rootDir>/__jest__/",
    "collectCoverageFrom": [
      "src/**/**/*.{js,jsx}",
      "!**/_mocks_/**",
      "!**/__tests__/**",
      "!**/assets/**"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "\\.snap$"
    ],
    "setupFiles": [
      "<rootDir>/__jest__/setup.js"
    ]
  }
}
